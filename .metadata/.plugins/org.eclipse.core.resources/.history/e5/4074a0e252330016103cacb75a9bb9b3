package br.com.ranking;

import java.util.Date;

/**
 * Classe que representa um evento
 * ex: Mark killed Fulado. 
 *
 *
 * @author Kaiqui Lopes <kaiquilopes@gmail.com.br>
 * @since 15 de jun de 2016 20:20:32
 * @version 1.0
 */
public class Evento{
	
	private String arma;
	private String causa;
	private Date data;
	private Jogador matou;
	private Jogador morreu;
	
	
	public String getArma() {
		return arma;
	}
	public void setArma( String arma ) {
		this.arma = arma;
	}
	public String getCausa() {
		return causa;
	}
	public void setCausa( String causa ) {
		this.causa = causa;
	}
	public Date getData() {
		return data;
	}
	public void setData( Date data ) {
		this.data = data;
	}
	public Jogador getMatou() {
		return matou;
	}
	public void setMatou( Jogador matou ) {
		this.matou = matou;
	}
	public Jogador getMorreu() {
		return morreu;
	}
	public void setMorreu( Jogador morreu ) {
		this.morreu = morreu;
	}
	
	

}
